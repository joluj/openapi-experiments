{
  "extends": ["../../.eslintrc.json"],
  "plugins": ["eslint-plugin-unused-imports"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "unused-imports/no-unused-imports": "error"
      }
    },
    {
      "files": ["src/app/types/**/*.ts"],
      "rules": {
        "@typescript-eslint/no-empty-function": "off"
      }
    }
  ]
}
